                                                <fo:list-item space-after="0em" space-before="0em"><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"/></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0"><fo:inline><fo:inline-container><fo:block-container margin="0" color="rgb(81 , 133 , 203)"><fo:block end-indent="0" start-indent="0"><fo:basic-link show-destination="new" indicate-destination="true" fox:alt-text="Learn to quickly replace content references and keyword references with relevant content in oXygen XML Editor." internal-destination="unique_14">Resolving conrefs and
                                                  keyrefs</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" fox:alt-text="Learn to quickly replace content references and keyword references with relevant content in oXygen XML Editor." internal-destination="unique_14"><fo:inline font-style="italic"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_14"> (on page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_14"><fo:page-number-citation ref-id="unique_14"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_14">)</fo:basic-link></fo:inline></fo:basic-link></fo:block></fo:block-container></fo:inline-container></fo:inline></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
